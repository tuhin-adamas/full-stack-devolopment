<html>
<head>
<title>

</title>
    <style>
        body{
            background: #242323;
            background: linear-gradient(90deg, rgba(36, 35, 35, 1) 0%, rgba(72, 72, 219, 1) 100%);
        }
        #name{
            color: white;
        }
        h1{
            color:white; ;

        }
        #div{
            background: #e0e02f;
            background: linear-gradient(90deg, rgba(224, 224, 47, 1) 0%, rgba(214, 72, 219, 1) 100%);
height: 65px;
        }
        .animated-table {
            width: 100%;
            border-collapse: collapse;
            font-family: sans-serif;
            margin-top: 20px;
        }

        .animated-table th,
        .animated-table td {
            border: 1px solid #ddd;
            padding: 12px;
            text-align: left;
        }

        .animated-table tbody tr {
            opacity: 0;
            transform: translateY(20px);
            animation: fadeInUp 0.5s ease forwards;
        }

        .animated-table tbody tr:nth-child(1) {
            animation-delay: 0.2s;
        }
        .animated-table tbody tr:nth-child(2) {
            animation-delay: 0.4s;
        }
        .animated-table tbody tr:nth-child(3) {
            animation-delay: 0.6s;
        }

        @keyframes fadeInUp {
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }
        .lg{
       background-color: rgba(255, 255, 255, 0.8);
            color: red;
            height: 50px;
            width: 400px;
            border: 4px solid #ada623;
        }
        .lg1{
            height: 50px;
            background: #55b322;
            background: linear-gradient(90deg, rgba(85, 179, 34, 1) 35%, rgba(52, 201, 97, 1) 100%);
            width: 150px;
            border: 4px solid #ada623;
        }
        .lg2{
            height: 50px;
            background: #b3240b;
            background: linear-gradient(90deg, rgba(179, 36, 11, 1) 35%, rgba(242, 46, 24, 1) 100%);
            width: 150px;
            border: 4px solid #ada623;
        }

        footer {
            align-items: center;
        }
        img{
            height: 200px;
            width: 200px;
            position: relative;
            top: 90px;
        }

    </style>
</head>
<body >

{{# if status}}
    <script>
        const status={{status}}
        if(status==1){
            alert("Data Updated Successfully ! ")
        }
        if(status==2){
            alert("Data Deleted Successfully !")
        }

    </script>
{{/if}}<div id="div">
<marquee>
    <h1> REGISTRATION LIST FOR UPCOMING SIGNIFYA EVENT</h1></marquee></div>>


<table border="2px" class="animated-table">
    <tr>
        <td id="name">NAME</td>
        <td STYLE="color: #dddddd"> ROLL</td>

        <td STYLE="color: #dddddd"> update</td>
        <td STYLE="color: #dddddd"> delete</td>
    </tr>
    {{#each data}}
        <form method="post" action="/update">
            <tr>
                <input class="lg" name="id" hidden type="text" value="{{this._id}}">
                <td>
                    <input class="lg"  type="text" name="name" value="{{this.name}}">
                </td>
                <td>
                    <input class="lg"  type="text" name="roll" value="{{this.roll}}">
                </td>

                <td>
                    <input class="lg1" name="btn" type="submit" value="UPDATE">
                </td>
                <td>
                    <input class="lg2"  name="btn" type="submit" value="DELETE">
                </td>
            </tr>
        </form>
    {{/each}}
</table><center>
<footer>
<div class="uhi">
    <img src="data:image/jpeg;base64,">
</div>
</footer></center>
</body>
</html>